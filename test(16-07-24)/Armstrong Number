class ArmstrongNum{
   public static void main(String[] args){
      int n = 153, rem, res= 0;
      int on = n;
      while(n != 0){
        rem = n % 10;
        res += rem * rem * rem;
        n /= 10;
       }
      if(res == on){
        System.out.println("it is an Armstrong number.");
      }
      else{
        System.out.println("it is not an Armstrong number.");
      }
   }
}
