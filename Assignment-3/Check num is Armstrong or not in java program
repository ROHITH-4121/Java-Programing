class Armstrong{
   public static void main(String[] args){
      int num = 153;
      int temp, total = 0;
      temp = num;
      while(temp != 0){
        int digit = temp % 10;
        total = total + digit * digit * digit;
        temp = temp / 10;
     }
     if(total == num){
        System.out.println(num+ "is an Armstrong number.");
     }else{
        System.out.println(num+ "is not an Armstrong number.");
    }
  }
}
